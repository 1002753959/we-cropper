.croper-wrapper{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    justify-content: center;
    height: 100%;
    background-color: #e5e5e5;
}
.cropper{
    background-color: rgba(0,0,0,.8);
}

.getCropperImage{
    position:absolute;
    bottom:40rpx;
    left:50%;
    transform:translateX(-50%);
    width:80%;
    color:#fff;
    background-color:#3CC51F;
}

.upload{
    position:absolute;
    bottom:150rpx;
    left:50%;
    transform:translateX(-50%);
    width:80%;
    color:#fff;
    background-color:#3CC51F;
}
